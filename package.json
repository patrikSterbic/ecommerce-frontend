{
  "name": "ecommerce-frontend-part",
  "productName": "EcommerceReactApp",
  "version": "0.1.0",
  "description": "Frontend part of ecommerce React app",
  "dependencies": {
    "antd": "^3.2.2",
    "attr-accept": "^1.1.2",
    "autoprefixer": "7.1.6",
    "axios": "^0.17.1",
